{
    "name": "zedsh/ZAdmin",
    "description": "",
    "keywords": [
        "ZAdmin"
    ],
    "homepage": "https://github.com/spatie/ZAdmin",
    "license": "MIT",
    "authors": [
        {
            "name": "Zaboev Eduard",
            "email": "zedsh@icloud.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Zedsh\\ZAdmin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zedsh\\ZAdmin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "zadminServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
